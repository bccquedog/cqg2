{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/next" }],
  "env": {
    "NODE_VERSION": "20.11.1",
    "ENABLE_EXPERIMENTAL_COREPACK": "1",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "/404.html" }
  ]
}

